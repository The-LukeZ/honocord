    "@discordjs/rest": "^2.6.0",
    "discord-api-types": "^0.38.37",
    "discord-interactions": "^4.4.0",
    "hono": "^4.11.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "prettier": "^3.7.4",
    "tsdown": "0.19.0-beta.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
